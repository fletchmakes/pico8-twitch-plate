pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
	x = 0
	y = 0
end

function _update()
	if(btn(⬅️)) then
		x -= 8
		if (x <= 0) then
			x = 128
		end
	end
	
	if(btn(➡️)) then
		x += 8
		if (x >= 128) then
			x = 0
		end
	end
	
	if(btn(⬆️)) then
		y -= 8
		if (y <= 0) then
			y = 128
		end
	end
	
	if (btn(⬇️)) then
		y += 8
		if (y >= 128) then
			y = 0
		end
	end
end

function _draw()
	cls(0)
	circfill(x, y, 16, 8)
end
